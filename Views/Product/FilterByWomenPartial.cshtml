@model List<ProjectShoeShop.Models.Category>

@if (Model.Count() > 0)
{
    <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
        <div class="panel-body">
            <ul>
                @foreach (var item in Model)
                {
                    <li><a href="/Product/ProductByWomenCategory?id=@item.Id">@item.Name</a></li>
                }
            </ul>
        </div>
    </div>
}
else
{
<p>
    Product not found
</p>
}